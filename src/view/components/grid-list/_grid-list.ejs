<%
// grid-list/_grid-list.ejs
// A main HTML file of "grid-list" component
//
  const Block = 'grid-list';
  const EjsRoot = '../../';
  if (typeof gridListParams === 'undefined') { gridListParams = {}; }
  const params = gridListParams;

  let dataParams = {};
  // Un-comment below to include a component specific data
  if (typeof params.type === 'undefined') { params.type = 'default'; }
  include(`./_${Block}-data`);
  dataParams = getGridListData(params.type);

  let defaults = {
    modifier: '',
    items: []
  };

  const p = util.init()(util, params, defaults, dataParams);
%>
<!-- <%=Block%> -->
<ul class="<%=Block%><%-util.setModifier(Block, p.modifier);%>">

</ul>
<!-- /<%=Block%> -->
